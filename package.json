{
  "name": "react-meui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "commit": "git-cz",
    "start": "father doc dev",
    "build:doc": "father doc build",
    "deploy": "father doc deploy",
    "build": "father build",
    "test": "father test",
    "test:coverage": "father test --coverage",
    "publish": "lerna publish",
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "packages/**/*.{ts,tsx,js,jsx}": [
      "eslint"
    ],
    "packages/**/*.{css,scss,less}": [
      "stylelint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "author": "xiaoma",
  "license": "ISC",
  "peerDependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "@umijs/fabric": "^2.0.4",
    "commitizen": "^3.1.2",
    "cz-conventional-changelog": "^2.1.0",
    "father": "^2.29.2",
    "husky": "^1.3.1",
    "lerna": "^3.16.4",
    "lint-staged": "^8.2.1",
    "prettier": "^1.19.1",
    "stylelint": "^10.0.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.7"
  }
}
